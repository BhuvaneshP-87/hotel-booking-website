pipeline{
    agent any
    tools {nodejs "Ease-Rooms"}

    stages{
        stage('Compile Stage'){
            steps{
                bat 'npm install'
                bat 'npm i package.json'
            }
        }

        stage('Testing'){
            steps{
                bat 'npm test'
            }
        }

        stage('Deploying'){
            steps{
                bat 'git push heroku master'
            }
        }


    }
}