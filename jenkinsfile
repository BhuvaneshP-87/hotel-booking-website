pipeline{
    agent any
    tools {nodejs "Ease-Rooms"}

    stages{
        stage('Compile Stage'){
            steps{
                sh 'npm install'
                sh 'npm i package.json'
                sh 'nodemon app.js'
            }
        }

        stage('Testing'){
            steps{
                sh 'npm test'
            }
        }
    }
}